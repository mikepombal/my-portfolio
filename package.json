{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "0.1.3",
  "scripts": {
    "dev": "next dev -p 3500",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "generate": "graphql-codegen --config codegen.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^4.0.0",
    "@graphql-codegen/cli": "^1.17.4",
    "@graphql-codegen/introspection": "^1.17.4",
    "@graphql-codegen/typescript": "^1.17.4",
    "@graphql-codegen/typescript-operations": "^1.17.4",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "bcryptjs": "^2.4.3",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "jsonwebtoken": "^8.5.1",
    "next": "10.0.3",
    "next-iron-session": "^4.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.0.8",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.2.3",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "postcss": "^8.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
